<%@ page language="java" pageEncoding="UTF-8"%>

<%
	String contextPath = request.getContextPath();
%>
<!-- easyui样式文件 -->
<link rel="stylesheet" type="text/css" href="<%=contextPath%>/base/easyui/themes/default/easyui.css">

<!-- easyui图标样式文件 -->
<link rel="stylesheet" type="text/css" href="<%=contextPath%>/base/easyui/themes/icon.css">

<!-- jQuery的js文件 -->
<script type="text/javascript" src="<%=contextPath%>/base/easyui/jquery.min.js"></script>

<!-- easyui的js文件 -->
<script type="text/javascript" src="<%=contextPath%>/base/easyui/jquery.easyui.min.js"></script>

<!-- 中文语言文件 -->
<script type="text/javascript" src="<%=contextPath%>/base/easyui/locale/easyui-lang-zh_CN.js"></script>
<script type="text/javascript" charset="utf-8"></script>

<!-- 扩展easyui的一些验证，如控制输入长度的验证等 -->
<script type="text/javascript" src="<%=contextPath%>/base/easyui/jquery.easyui.extend.js"></script>



